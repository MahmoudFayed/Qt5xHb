$(QT5XHB_OBJ_DIR)/%.obj : $(QT5XHB_SRC_DIR)/QtXmlPatterns/%.cpp
	$(CC) $(CFLAGS) $(CINCLUDES) -Fo$@ $^

$(QT5XHB_OBJ_DIR)/%.obj : $(QT5XHB_OBJ_DIR)/%.c
	$(CC) $(CFLAGS) $(CINCLUDES) -Fo$@ $^
$(QT5XHB_OBJ_DIR)/%.obj : $(QT5XHB_OBJ_DIR)/%.cpp
	$(CC) $(CFLAGS) $(CINCLUDES) -Fo$@ $^

